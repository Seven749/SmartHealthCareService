<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sevenrecy.smarthealthcareservice.dao.NurseDao">
    <select id="selectNurse" parameterType="String" resultType="Nurse">
        SELECT * FROM nurse WHERE nur_id=#{nur_id} AND pwd=#{pwd}
    </select>
</mapper>